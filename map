import java.util.*;

public class MapEg  {
  
	public static void main(String args[])  {
	HashMap map=new HashMap();
	
	map.put("One","1st");
	map.put("Second",new Integer(2));
	map.put("Third","3rd");
	map.put("Third","III");
	
 	Set set1=map.keySet();

	Collection collection=map.values();

	Set set2=map.entrySet();
	
	System.out.println(set1 + "\n" + collection + "\n" + set2);
   }
}
